<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.pre.team.uoffice.domain.Visit"
		table="t_visit">
		<id name="visitId" type="java.lang.Integer">
			<column name="visit_id" />
			<generator class="identity" />
		</id>
		<property name="visitIp" type="java.lang.String">
			<column name="visit_ip"></column>
		</property>
		<property name="visitTime" type="java.util.Date">
			<column name="visit_time"></column>
		</property>
	</class>
</hibernate-mapping>

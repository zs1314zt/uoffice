<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.pre.team.uoffice.domain.Order"
		table="t_order">
		<id name="orderId" type="java.lang.Integer">
			<column name="order_id" />
			<generator class="identity" />
		</id>
		<property name="officeId" type="java.lang.Integer">
			<column name="office_id"></column>
		</property>
		<property name="officeName" type="java.lang.String">
			<column name="office_name"></column>
		</property>
		<property name="phone" type="java.lang.String">
			<column name="phone"></column>
		</property>
		<property name="email" type="java.lang.String">
			<column name="email"></column>
		</property>
		<property name="orderTime" type="java.util.Date">
			<column name="order_time"></column>
		</property>
	</class>
</hibernate-mapping>
